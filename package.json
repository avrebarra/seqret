{
  "name": "seqret",
  "version": "1.1.0",
  "description": "Secrets as protected QRs",
  "homepage": "https://avrebarra.github.io/seqret",
  "source": "src/index.html",
  "pwaManifest": {
    "shortName": "Seqret",
    "theme": "#FFFFFF",
    "generateIconOptions": {
      "baseIcon": "./public/icon.png",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build && workbox generateSW",
    "remotedeploy": "rm -rf ./dist && parcel build --public-url ./ --no-source-maps && workbox generateSW && gh-pages -d dist"
  },
  "repository": "github.com/avrebarra/seqret",
  "author": "Avreghly Barra <avreghly@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@parcel/config-default": "^2.5.0",
    "@parcel/core": "^2.5.0",
    "@parcel/packager-raw-url": "2.5.0",
    "@parcel/packager-xml": "2.5.0",
    "@types/crypto-js": "^4.0.2",
    "@types/node": "^17.0.5",
    "@types/node-rsa": "^1.1.1",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-measure": "^2.0.8",
    "@types/styletron-engine-atomic": "^1.1.1",
    "@types/styletron-react": "^5.0.3",
    "@types/styletron-standard": "^2.0.2",
    "autoprefixer": "^10.4.0",
    "parcel": "^2.5.0",
    "parcel-config-pwa-manifest": "^0.1.1",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.7"
  },
  "dependencies": {
    "baseui": "^10.6.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "html5-qrcode": "^2.1.6",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazily": "^0.9.1",
    "react-measure": "^2.5.2",
    "react-router-dom": "^6.2.1",
    "styletron-engine-atomic": "^1.4.8",
    "styletron-react": "^6.0.2"
  }
}
